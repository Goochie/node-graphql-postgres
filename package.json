{
  "name": "smart-life-path-api-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "wpbuild": "nest build --watch --webpack",
    "wpstart": "node dist/main",
    "docker:db": "docker-compose -f dev-db-doker.yaml up",
    "build": "rimraf dist && tsc -p tsconfig.build.json && npm run copy:env && npm run copy:templates",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "nest": "nest",
    "migrate": "npm run migrate:run",
    "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "migrate:generate": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:generate -n ",
    "migrate:run": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:revert": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:revert",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect=7000 -r ts-node/register src/main.ts\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "START=1 node dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "postinstall": "npm run migrate",
    "copy:env": "cp -r 'env' 'dist/env'",
    "copy:templates": "cp -r 'email-templates' 'dist/email-templates'"
  },
  "dependencies": {
    "@nest-modules/mailer": "^1.1.3",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/graphql": "^6.6.2",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/platform-socket.io": "^7.0.7",
    "@nestjs/typeorm": "^6.1.3",
    "@nestjs/websockets": "^7.0.7",
    "@sentry/node": "^5.11.1",
    "@types/sharp": "^0.23.0",
    "@types/stripe": "^7.13.19",
    "apollo-server-express": "^2.12.0",
    "aws-sdk": "^2.536.0",
    "bcrypt": "^3.0.6",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.0",
    "cordova": "8.1.2",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.0.0",
    "express-rate-limit": "^5.1.1",
    "global": "^4.4.0",
    "graphql": "^14.6.0",
    "graphql-tools": "^4.0.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "luxon": "^1.19.3",
    "moment": "^2.24.0",
    "mysql": "^2.17.1",
    "nest-schedule": "^0.6.4",
    "nodemailer": "^6.4.6",
    "nodemailer-mailgun-transport": "^1.4.0",
    "nodemon": "^1.19.2",
    "pg": "^7.18.2",
    "postgres-date": "^1.0.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.2",
    "rxjs": "^6.5.2",
    "sharp": "^0.23.2",
    "stripe": "^7.15.0",
    "type-graphql": "^0.17.4",
    "typeorm": "^0.2.18",
    "typeorm-seeding": "^1.0.0-rc.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@nestjs/testing": "6.1.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "4.16.1",
    "@types/graphql": "^14.2.3",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.4",
    "@types/supertest": "2.0.7",
    "jest": "24.7.1",
    "prettier": "1.17.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.7",
    "webpack": "^4.41.2",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
